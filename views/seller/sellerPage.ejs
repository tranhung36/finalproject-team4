<div class="col-lg-6">
    <form action="/sellerPage/crud-page/createProduct" method="POST">
        <div class="input-group">
            <input class="form-control form-control-lg" name="haha" type="text" placeholder="Enter your email address">
            <button class="btn btn-dark" id="button-addon2" type="submit">Subscribe</button>
        </div>
    </form>
</div>
